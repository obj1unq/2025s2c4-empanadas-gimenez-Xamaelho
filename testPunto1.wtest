import empleados.*

test "punto1: Planteo inicial" {

  baigorria.cargarEmpanadas(500)
  galvan.cambiarSueldo(20000)
  baigorria.cargarEmpanadas(300)
  assert.equals(12000, baigorria.sueldo())
  assert.equals(20000, galvan.sueldo())
  
  gimenez.pagarEmpleado(baigorria)
  gimenez.pagarEmpleado(galvan)
  assert.equals(268000, gimenez.fondoSueldo())
  
}

